<h1>会員登録</h1>

<%=form_for(@user,url:signin_path)do |f|%>

<%=f.label :email%>
<%=f.text_field :email , class: "form__txt"%>

<%=f.label :password%>
<%=f.text_field :password , class: "form__txt"%>

<%=f.label :password_confirmation%>
<%=f.text_field :password_confirmation , class: "form__txt"%>

<%=f.label :twitter_accoount%>
<%=f.text_field :twitter_account , class: "form__txt"%>

<%=f.label :image_name%>
<%=f.text_field :image_name , class: "form__txt"%>

<%=f.submit "登録" %>
<%end%>

<%if session[:user_id] != nil%>
<%=User.find(session[:user_id]).email%>
<%end%>

